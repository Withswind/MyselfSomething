body
	background #fafafa
	font-family: Helvetica,STHeiti,Droid Sans Fallback;
.tod-hot
	position fixed
	right 0.625rem
	bottom 5rem
	width 3rem
	height 3.5rem
	background url(http://img1.gtimg.com/rcdimg/20160810/10/13385664.png) no-repeat
	background-size 100%
	z-index 999
.fix
	position fixed
	bottom 0
	left 0
	width 100%
	height 2.8125rem
	background-color rgba(0,0,0,.5)
	padding 0.5rem 0.9375rem
	z-index 999
	& .le
		width 1.875rem
		height 1.875rem
		float left
		background-image url(http://mat1.gtimg.com/sports/sportAppWeb/kbsshare/statics/kbs-logo_cf8381.png)
		background-size cover
	& .cen
		line-height 1rem
		margin-left 0.5rem
		float left
		color #fafafa
		font-family "Microsoft Yahei"
		 margin-top -0.0625rem
		& h2
			font-size 0.9375rem
		& p	
			font-size 0.75rem	
	& .ri
		float right
		border-radius 0.125rem
		width 3.625rem
		height 1.875rem
		background-color #4998E7
		color #fff
		font-size 0.75rem
		font-family "Microsoft Yahei"
		line-height 1.875rem
		text-align center
		margin-right 1.125rem
	& .la
		float right
		width 0.75rem
		height 0.75rem
		background url(http://mat1.gtimg.com/www/mobi/image/app_close.png) no-repeat
		background-size 100%
		margin 0.5rem 0 0 0
.global
	background #fafafa
	header
		width 100%
		height 2.75rem
		background-image url(http://mat1.gtimg.com/www/mobi/image/headerbg_v2.png)
		background-repeat no-repeat
		background-position 8.6875rem top
		background-size 8.8125rem 2.75rem
		font-size 0
		border-bottom 0.125rem solid #3e86ce
		
		& a.tit-icon
			display block
			float left
			width 7.4375rem
			height 1.875rem
			background-image url(http://mat1.gtimg.com/www/mobi/image/logo/logo_sports.png)
			background-size cover
			margin 0.4375rem 0 0 0.625rem
		& a.right
			display block
			float right
			margin 0.875rem 0.875rem 0 0
			width 1.375rem
			height 1rem
			background-image url(http://mat1.gtimg.com/www/mobi/image/icon.png)
			background-size 7.4375rem 1.3125rem
			background-position -1.25rem 0
	section
		width 100%
		& .choose
			width 100%
			padding 0.3125rem 0 0.3125rem 0
			background-color #3e86ce
			& .tit-out
				height 1.875rem
				& a
					display block
					font-size 1.125rem
					float left
					width 20%
					height 1.875rem
					line-height 1.875rem
					text-align center
					color #fff
					&.cur
						background #226fbb
		& .banner
			width 100%
			max-height 10.3125rem 
			position relative
			overflow hidden
			text-align center
			& .img
				max-width 100%
			& h2
				width 100%
				text-align center
				padding 0 2rem 0 0
				position absolute
				font-size 1rem
				color #fafafa
				left 0
				bottom 0
				height 2rem
				line-height 2rem
				text-shadow 1px 1px 1px #000
				background -webkit-linear-gradient(top,rgba(0,0,0,0),rgba(0,0,0,.9));
		.medal
			width 100%
			padding 0.3125rem 0.4375rem 0
			& .tit-bg
				width 100%
				border 1px solid #CDDEF0
				border-bottom transparent
				height 1.5625rem
				background url(http://mat1.gtimg.com/www/mobi/image/jpb_hd.png) no-repeat
				background-size cover
				
				p
					font-size 0.875rem
					font-weight bold
					line-height 1.5625rem
					padding-left 0.375rem
					font-family "Microsoft Yahei"
			& .medalNum
				width 100%
				border 1px solid #CDDEF0
				border-top transparent
				height 2rem
				padding-bottom 0.125rem
				overflow hidden
				& .selector
					width 100%
					background-color #fff
					position relative 
					z-index 100
					font-family "Microsoft Yahei"
					font-size 0.875rem
					color #010101 
					animation rot 10s 3s infinite linear
					top -1.875rem
					& .pub
						width 100%
						display flex
						flex-flow row
						& li
							text-align center
							height 1.875rem
							line-height 1.875rem
							position relative
							&:before
								content ""
								display inline-block
								position absolute
								height 0.9375rem
								width 1px
								left -1px
								top 0.4375rem
								background-color #DCDCDC
								
						& li:nth-child(2)
							flex-basis 4.1875rem
						& li:nth-child(3),& li:nth-child(4),& li:nth-child(5)
							flex-basis 2.875rem
						& li:nth-child(6)
							flex-basis 3.1875rem
						& li:first-child
							flex-basis 3rem
							&:before
								display none
							& span
								display inline-block
								width 1rem
								height 1rem
								background-color #60bb46
								border-radius 0.1875rem
								color #fff
								font-size 0.75rem
								line-height 1rem
								vertical-align middle
								margin-top -0.1875rem
						& li.pub-li
							& i
								display inline-block 
								width 0.875rem
								height 0.875rem
								background-image url(http://mat1.gtimg.com/www/mobi/image/rank_icons2.png)
								background-size 7.25rem 0.8125rem
								vertical-align middle
								margin-top -0.25rem
								margin-right 0.1875rem
								background-repeat no-repeat
							& i.gold
								background-position 0 0
							& i.silver
								background-position -3.375rem 0
							& i.cop
								background-position -6.375rem 0
					& .one
						& li:first-child
							& span
								background-color #ff8327
			& a.ad
				margin-top 0.3125rem
				display block
				width 100%
				border 1px solid #b5dcf2
				color #0695d7
				height 2.25rem
				padding-left 0.875rem
				background-color #EDFAFF
				& img
					height 2.125rem
					vertical-align middle
					margin-right 0.125rem
				& span
					font-size 0.875rem
					font-family "Microsoft Yahei"
					font-weight bold
	.MainInner
		width 100%
		padding 0.25rem 0.4375rem 0
		& a.pub
			display block
			width 100%
			position relative
			padding 0.625rem 0
			border-bottom 1px solid #eceef0
			& img
				 padding 1px 0.1875rem
				 border 1px solid #e0e0e0
				 float left
				 width 4.9375rem
				 height 3.6875rem
			& h2
				font-size 0.9375rem
				overflow hidden
				white-space nowrap
				text-overflow ellipsis
				height 1.375rem
				line-height 1.375rem
				padding-left 0.3125rem
				font-weight 400
			& p
				padding-left 5.25rem
				font-size 0.8125rem
				margin-top 0.25rem
				line-height 1.0625rem
				color #878c92
			& span.discuss
				position absolute
				right 0
				bottom 0.25rem
				color #878c92
				& i.pub-i
					display inline-block
					width 0.75rem
					height 0.75rem
					background-image url(http://mat1.gtimg.com/www/mobi/image/icon.png)	
					background-position -5.25rem 0.125rem
					background-size 7.4375rem 1.3123rem
					vertical-align middle
				& i.spec
					display inline-block
					width 3rem
					height 1.75rem
					background-color #4886da
					font-size 1.125rem
					line-height 1.75rem
					text-align center
					color #fafafa
					transform scale(0.5)
					vertical-align middle
					border-radius 0.25rem 
					position relative
					top 0.125rem
				& span
					display inline-block
					font-size .6875em
	.more
		height 2.1875rem
		line-height 2.1875rem
		margin 0 0.5rem 0.9375rem
		border 1px solid #efefef
		background-color #f6f5f5
		& p
			text-align center
			font-size 1rem
			color #b4b4b4
			&:after
				content ""
				display inline-block
				width 1.3125rem
				height 1.3125rem
				background-image url(http://mat1.gtimg.com/www/mobi/image/icon.png)
				background-size 7.4375rem 1.3125rem
				background-position -2.625rem 0
				vertical-align middle
				margin-left 0.3125rem
				margin-top -0.125rem		
	.bot,.fiden
		width 100%
		overflow hidden
		& ul
			& li
				float left
				width 20%
				height 1.75rem
				text-align center			
	.bot
		padding 0.75rem 0
		background #49535d	
		color #95a0ac
		line-height 1.75rem
		font-size 0.9375rem
	.fiden
		background #4998e7
		color #fff
		line-height 1.875rem
		font-size 1.125rem
		height 0rem
		overflow hidden
		transition all 0.3s
		.add
			height 0.75rem
	footer
		background-color #404a54
		padding 0.625rem 0 0.75rem 0.5rem
		overflow hidden
		position relative
		font-size 0.8125rem
		color #95a0ac 
		line-height 1.5625rem
		& .cen,.co
			display block
		& .ba
			color #95a0ac
			&:before
				content ""
				background #565e61
				width 1px
				height 0.75rem
				display inline-block
				vertical-align -1px
				font-size 1em
				color #565e61
				margin-right 0.75rem
				margin-left 0.75rem
		& .co
			font-size 0.5rem
			line-height 1
			white-space nowrap
			color #707982
@keyframes rot
	5%
		top -3.75rem
	33.333%
		top -3.75rem
	38.333%
		top -5.625rem
	66.666%
		top -5.625rem
	71.666%
		top -7.5rem
	100%
		top -7.5rem
				