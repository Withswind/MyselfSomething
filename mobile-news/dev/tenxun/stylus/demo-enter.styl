body
	background #fafafa
	font-family Helvetica,STHeiti,Droid Sans Fallback
	font-size 1rem
	background #eee
	height 100%
.global
	header
		div.icon-big
			width 100%
			height 3.875rem
			background-image url(http://mat1.gtimg.com/www/webapp/images/shipei_logo.png)
			background-size 15.25rem 6.25rem
			background-repeat no-repeat
			background-position 50% 50%
			margin-top 3.125rem
	section
		width 100%
		padding 2.4375rem 0.9375rem 0
		& .pubIn-big
			width 100%
			background-color #fff
			padding-left 1rem
			position relative
			input
				width 100%
				font-size 1rem
				height 2.75rem
				position relative
				outline none
			.focus-none
				width 2.75rem
				height 2.75rem
				position absolute
				background-color #fff
				right 0
				top 0
				z-index 1
				border-radius 0 0.375rem 0.375rem 0 
				i.icon
					display block
					margin auto
					margin-top 0.8125rem
					width 1.125rem
					height 1.125rem
					background url("../../../img/images/12.png") no-repeat -7.3125rem -0.125rem
		& .input
			height 2.8125rem
			border-bottom 1px solid rgb(234, 234, 234)
			border-radius 0.375rem 0.375rem 0 0
		& .key
			height 2.75rem
			border-radius 0 0 0.375rem 0.375rem
		& .go
			margin-top 0.9375rem
			width 100%
			height 2.75rem
			background-color rgb(20, 110, 223)
			border-radius 0.375rem
			line-height 2.75rem
			font-size 1rem
			text-align center
			color #fff
		& .quick-go
			background-color #e7e7e7
			border 1px solid #9abbe3
			color rgb(20, 110, 223)
	footer
		color  #246183
		padding 0 0.9375rem
		margin-top 1.875rem
		font-size 0.875rem
		.forget
			float left
		.rigister
			float right
		
			
			
			
			
				
				
				
			
			